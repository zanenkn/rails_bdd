<h1>Articles</h1>
<% @articles.each do |article| %>
  <div id= <%= article.id %> >
    <h2><%= article.title %></h3>
    <p><%= article.content %></p>
    <%= link_to 'View', article_path(article) %>
  </div>
<% end %>

